{
  "name": "azure-ad-jwt-strategy",
  "version": "1.0.3",
  "description": "Azure AD JWT authentication strategy for NestJS applications with multi-tenant support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "keywords": [
    "nestjs",
    "azure",
    "authentication",
    "jwt",
    "multi-tenant"
  ],
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/passport": "^10.0.0"
  },
  "dependencies": {
    "passport-jwt": "^4.0.1",
    "jwks-rsa": "^3.1.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@types/passport-jwt": "^3.0.10",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0",
    "prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0"
  },
  "author": "Maulik Sompura",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lazzy_ms/azure-ad-jwt-strategy"
  },
  "homepage": "https://github.com/lazzy_ms/azure-ad-jwt-strategy#readme"
}
